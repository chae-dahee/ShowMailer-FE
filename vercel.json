{
  "version": 2,
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "https://demo-be-navy.vercel.app/$1",
      "methods": ["GET", "POST", "PUT", "DELETE"],
      "headers": {
        "X-Forwarded-Host": "https://dev-alarm-fe.vercel.app/"
      }
    }
  ]
}
